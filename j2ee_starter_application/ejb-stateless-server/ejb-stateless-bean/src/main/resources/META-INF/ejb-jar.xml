<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar
  xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  version="3.2"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/ejb-jar_3_2.xsd"
>
    <enterprise-beans>
        <session>
            <ejb-name>HostManagementSessionBean</ejb-name>
            <mapped-name>phamsodiep/ejb/HostManagementSessionBean</mapped-name>
            <business-local>org.phamsodiep.skeleton.HostManagementSessionBeanLocal</business-local>
            <business-remote>org.phamsodiep.skeleton.HostManagementSessionBeanRemote</business-remote>
            <ejb-class>org.phamsodiep.skeleton.HostManagementSessionBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
    </enterprise-beans>
</ejb-jar>


